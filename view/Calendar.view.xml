<mvc:View controllerName="ypglmasterdetailportal.controller.Detail" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc"
	xmlns:semantic="sap.m.semantic" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:core="sap.ui.core">
	<semantic:DetailPage id="page" navButtonPress="onNavBack" showNavButton="{device>/system/phone}" title="{i18n>detailTitle}"
		busy="{detailView>/busy}" busyIndicatorDelay="{detailView>/delay}">
		<semantic:content>
			<ObjectHeader id="objectHeader" title="{Text}" numberUnit="{LinkType}"></ObjectHeader>
			<!--			<IconTabBar
				id="iconTabBar"
				class="sapUiResponsiveContentPadding">
				<items>
					<IconTabFilter
						id="iconTabBarFilter1"
						icon="sap-icon://hint"
						tooltip="{i18n>detailIconTabBarInfo}">
					</IconTabFilter>
					<IconTabFilter
						id="iconTabBarFilter2"
						icon="sap-icon://attachment"
						tooltip="{i18n>detailIconTabBarAttachments}">
					</IconTabFilter>
				</items>
			</IconTabBar>-->
			<Panel headerText="" visible="{=${Key}==='HOME'}">
				<core:Fragment fragmentName="ypglmasterdetailportal.view.home" type="XML"/>
			</Panel>
			<Panel headerText="" visible="{=${Key}==='PROZESSBEGLEITUNG'}">
				<core:Fragment fragmentName="ypglmasterdetailportal.view.prozessbegleitung" type="XML"/>
			</Panel>
			<Panel headerText="" visible="{=${Key}==='FENG_SHUI'}">
				<core:Fragment fragmentName="ypglmasterdetailportal.view.fengShui" type="XML"/>
			</Panel>
			<Panel headerText="" visible="{=${Key}==='FOTOGRAFIE'}">
				<core:Fragment fragmentName="ypglmasterdetailportal.view.fotografie" type="XML"/>
			</Panel>
				<Panel headerText="" visible="{=${Key}==='KALENDER'}">
				<core:Fragment fragmentName="ypglmasterdetailportal.view.kalenderliste" type="XML"/>
			</Panel>
					<Panel headerText="" visible="{=${Key}==='PROFIL'}">
				<core:Fragment fragmentName="ypglmasterdetailportal.view.profil" type="XML"/>
			</Panel>
				<Panel headerText="" visible="{=${Key}==='KONTAKT'}">
				<core:Fragment fragmentName="ypglmasterdetailportal.view.kontakt" type="XML"/>
			</Panel>
				<Panel headerText="" visible="{=${Key}==='LINKS'}">
				<core:Fragment fragmentName="ypglmasterdetailportal.view.links" type="XML"/>
			</Panel>
			
			
		</semantic:content>
		<semantic:sendEmailAction>
			<semantic:SendEmailAction id="shareEmail" press="onShareEmailPress"/>
		</semantic:sendEmailAction>
	</semantic:DetailPage>
</mvc:View>