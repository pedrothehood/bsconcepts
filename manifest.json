{
	"_version": "1.4.0",
	"sap.app": {
		"id": "ypglmasterdetailportal",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"mainService": {
				"uri": "data/workData.json",
				"type": "JSON",
				"settings": {
					"localUri": "localService/metadata.xml"
				}
			},
			"workDataSource": {
				"uri": "data/workData.json",
				"type": "JSON"
			},
			"init_kalender": {
				"uri": "data/kalender.json",
				"type": "JSON"
			}
		},
		"sourceTemplate": {
			"id": "sap.ui.ui5-template-plugin.2masterdetail",
			"version": "1.38.3"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://detail-view",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "ypglmasterdetailportal.view.App",
			"type": "XML",
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.38.0",
			"libs": {
				"sap.ui.core": {
					"minVersion": "1.38.0"
				},
				"sap.m": {
					"minVersion": "1.38.0"
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "ypglmasterdetailportal.i18n.i18n"
				}
			},
			"": {
				"dataSource": "mainService",
				"preload": true
			},
			"workData": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "workDataSource"
			},
			"kalender": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "init_kalender"
			}

		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "ypglmasterdetailportal.view",
				"controlId": "idAppControl",
				"controlAggregation": "detailPages",
				"bypassed": {
					"target": [
						"master",
						"notFound"
					]
				},
				"async": true
			},
			"routes": [{
				"pattern": "",
				"name": "master",
				"target": [
					"object",
					"master"
				]
			}, {
				"pattern": "BSCONCEPTS/{objectId}",
				"name": "object",
				"target": [
					"master",
					"object"
				]
			}, {
				"pattern": "BSCONCEPTS/KALENDER/{year}",
				"name": "calendar",
				"target": [
					"master",
					"calendar"
				]
			}],
			"targets": {
				"master": {
					"viewName": "Master",
					"viewLevel": 1,
					"viewId": "master",
					"controlAggregation": "masterPages"
				},
				"object": {
					"viewName": "Detail",
					"viewId": "detail",
					"viewLevel": 2
				},
				"calendar": {
					"viewName": "Calendar",
					"viewId": "calendar",
					"viewLevel": 3
				},
				"detailObjectNotFound": {
					"viewName": "DetailObjectNotFound",
					"viewId": "detailObjectNotFound"
				},
				"detailNoObjectsAvailable": {
					"viewName": "DetailNoObjectsAvailable",
					"viewId": "detailNoObjectsAvailable"
				},
				"notFound": {
					"viewName": "NotFound",
					"viewId": "notFound"
				}
			}
		}
	}
}